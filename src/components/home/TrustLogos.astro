<style>
  .trust-logos-section {
    padding: 40px 0 60px;
    background: var(--bg);
  }

  .trust-logos-label {
    text-align: center;
    font-size: 0.8rem;
    font-weight: 500;
    text-transform: uppercase;
    letter-spacing: 0.1em;
    color: var(--text-muted);
    margin-bottom: 24px;
  }

  .trust-logos {
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 40px;
    flex-wrap: wrap;
  }

  .logo-item {
    opacity: 0.4;
    transition: opacity 0.3s var(--ease-out), transform 0.3s var(--ease-out);
  }

  .logo-item:hover {
    opacity: 1;
    transform: scale(1.1);
  }

  .logo-item svg {
    width: 36px;
    height: 36px;
    color: var(--text-secondary);
  }

  @media (max-width: 768px) {
    .trust-logos {
      gap: 28px;
    }

    .logo-item svg {
      width: 28px;
      height: 28px;
    }
  }

  @media (max-width: 480px) {
    .trust-logos-section {
      padding: 32px 0 48px;
    }

    .trust-logos {
      gap: 24px;
    }

    .logo-item svg {
      width: 24px;
      height: 24px;
    }
  }
</style>
